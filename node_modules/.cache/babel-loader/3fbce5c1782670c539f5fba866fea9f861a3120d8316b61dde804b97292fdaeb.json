           this.$ref.from.resetFields();\n            this.dialogVisible = false;\n        },\n        cancel() {\n            this.handleClose();\n            this.$message('取消刪除');\n        },\n        confirm() { //提交表單判斷是哪種操作\n            if (this.operateType === 'edit'){\n                this.$http.post('/user/edit', this.operateForm).then(res => {\n                    console.log(res);\n                    this.dialogVisible = false;\n                    this.getList();\n                })\n            } else {\n                this.$http.post('/user/add', this.operateForm).then(res => {\n                    console.log(res);\n                    this.dialogVisible = false;\n                    this.getList();\n                })\n            }\n        },\n        addUser() {\n            this.dialogVisible= true;\n            this.operateType='add'\n            this.operateForm = {\n                name: '',\n                addr: '',\n                age: '',\n                birth: '',\n                sex: ''\n            }\n        },\n        editUser(row){\n            console.log(\"Editing user\", row);\n            this.operateType = 'edit';\n            console.log(\"Operate Type after setting:\", this.operateType);\n            this.dialogVisible = true;\n            this.operateForm = row;\n        },\n        delUser (row){\n            this.$confirm(\"此操作將永久刪除該資料，是否繼續？\", \"提示\", {\n                confirmButtonText: \"確認\",\n                cancelButtonText: \"取消\",\n                type: \"warning\"\n            }).then(() => {\n                const id = row.id;\n                return this.$http.post(\"user/del\", { id: id });\n            }).then(() => {\n                this.getList();\n                this.$message.success('用戶已刪除');\n            }).catch(err => {\n                console.log(\"刪除操作取消\", err);\n                \n            })\n        },\n        getList(name = ''){\n            console.log('Search keyword:', name);\n            this.config.loading = true;\n            name ? (this.config.page = 1) : '';\n            getUser({\n                page: this.config.page,\n                name\n            }).then(({data: res}) => {\n                console.log(res, 'res');\n                this.tableData = res.list.map(item => {\n                    item.sex = item.sex === 0 ? \"女\": \"男\"\n                    return item \n                })\n                this.config.total = res.count;\n                this.config.loading = false;\n            });\n        }\n    },\n    created() {\n        this.getList();\n    }\n}\n\n</script>\n\n<style lang=\"less\" scoped>\n    .manage-header {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n</style>"],"mappings":";;;EACSA,KAAK,EAAC;AAAQ;;;;;EAuBVA,KAAK,EAAC;AAAe;;;;;;;;;uBAvB9BC,mBAAA,CA0CM,OA1CNC,UA0CM,GAzCFC,YAAA,CAqBYC,oBAAA;IApBZC,KAAK,EAAC,IAAI;IACFC,OAAO,EAAEC,KAAA,CAAAC,aAAa;4DAAbD,KAAA,CAAAC,aAAa,GAAAC,MAAA;;IAenBC,MAAM,EAAAC,QAAA,CACb,MAAyD,CAAzDR,YAAA,CAAyDS,oBAAA;MAA7CC,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAL,MAAA,IAAEF,KAAA,CAAAC,aAAa;;wBAAU,MAAG,C,iBAAH,KAAG,E;;QAC7CL,YAAA,CAAiFS,oBAAA;MAAtEG,IAAI,EAAC,SAAS;MAAEF,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAL,MAAA,KAAEF,KAAA,CAAAC,aAAa,UAAUQ,QAAA,CAAAC,OAAO;;wBAAE,MAAG,C,iBAAH,KAAG,E;;;;sBAfzE,MASW,CATKC,IAAA,CAAAC,WAAW,e,cACvBC,YAAA,CAOUC,kBAAA;;MAPAC,KAAK,EAAEJ,IAAA,CAAAK;;wBACb,MAEe,CAFfpB,YAAA,CAEeqB,uBAAA;QAFDC,KAAK,EAAC,IAAI;QAACC,IAAI,EAAC;;0BAC1B,MAAgD,CAAhDvB,YAAA,CAAgDwB,mBAAA;sBAA7BT,IAAA,CAAAK,WAAW,CAACK,IAAI;qEAAhBV,IAAA,CAAAK,WAAW,CAACK,IAAI,GAAAnB,MAAA;;;UAEvCN,YAAA,CAEeqB,uBAAA;QAFDC,KAAK,EAAC,IAAI;QAACC,IAAI,EAAC;;0BAC1B,MAA+C,CAA/CvB,YAAA,CAA+CwB,mBAAA;sBAA5BT,IAAA,CAAAK,WAAW,CAACM,GAAG;qEAAfX,IAAA,CAAAK,WAAW,CAACM,GAAG,GAAApB,MAAA;;;;;;mDAK1CR,mBAAA,CAAmB,QAAA6B,UAAA,EAAb,QAAM,G;;kCAOhBC,mBAAA,CAUM,OAVNC,UAUM,GATF7B,YAAA,CAA+ES,oBAAA;IAApEG,IAAI,EAAC,SAAS;IAAEF,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAL,MAAA,KAAEF,KAAA,CAAAC,aAAa,SAASQ,QA