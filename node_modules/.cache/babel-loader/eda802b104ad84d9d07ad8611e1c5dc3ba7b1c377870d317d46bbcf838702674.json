{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport * as ElementPlusIconsVue from '@element-plus/icons-vue';\nexport default {\n  data() {\n    return {\n      menu: [{\n        path: '/home',\n        name: 'home',\n        label: '首頁',\n        icon: 'HomeFilled',\n        url: 'Home/Home'\n      }, {\n        path: '/mall',\n        name: 'mall',\n        label: '商品管理',\n        icon: 'video-play',\n        url: 'MallManage/MallManage'\n      }, {\n        path: '/user',\n        name: 'user',\n        label: '用戶管理',\n        icon: 'user',\n        url: 'UserManage/UserManage'\n      }, {\n        label: '其他',\n        icon: 'location',\n        children: [{\n          path: '/page1',\n          name: 'page1',\n          label: '頁面1',\n          icon: 'Setting',\n          url: 'Other/PageOne'\n        }, {\n          path: '/page2',\n          name: 'page2',\n          label: '頁面2',\n          icon: 'Setting',\n          url: 'Other/PageTwo'\n        }]\n      }]\n    };\n  },\n  methods: {\n    clickMenu(item) {\n      this.$router.push({\n        name: item.name\n      });\n      this.$store.commit('selectMenu', item);\n    },\n    getIconComponent(iconName) {\n      return ElementPlusIconsVue[iconName] || 'defaultIcon';\n    }\n  },\n  computed: {\n    filterMenu() {\n      return this.menu.filter(item => !item.children);\n    },\n    filterMenu_withChildren() {\n      return this.menu.filter(item => item.children);\n    },\n    isCollapsed() {\n      return this.$store.state.tab.isCollapsed;\n    }\n  }\n};","map":{"version":3,"names":["ElementPlusIconsVue","data","menu","path","name","label","icon","url","children","methods","clickMenu","item","$router","push","$store","commit","getIconComponent","iconName","computed","filterMenu","filter","filterMenu_withChildren","isCollapsed","state","tab"],"sources":["/Users/serina/Desktop/vue-backstage/src/components/CommonAside.vue"],"sourcesContent":["<template>\n  <el-menu\n   default-active=\"1\"\n   class=\"el-menu-vertical-demo\"\n   background-color=\"#545c64\"\n   text-color=\"#fff\"\n   active-text-color=\"#ffd04b\"\n >\n <h3>{{ isCollapsed ? '後臺' : '通用後臺管理系統'}}</h3>\n   <el-menu-item v-for=\"item in filterMenu\" :index=\"item.path\" :key=\"item.path\" @click=\"clickMenu(item)\">\n     <el-icon>\n       <component :is=\"item.icon\"></component>\n     </el-icon>\n     <template #title>{{ item.label }}</template>\n   </el-menu-item>\n   <el-sub-menu v-for=\"item in filterMenu_withChildren\" :index=\"item.path\" :key=\"item.path\">\n     <template #title>\n       <el-icon>\n         <component :is=\"item.icon\"></component>\n       </el-icon>\n       <span>其他</span>\n     </template>\n     <el-menu-item-group>\n       <el-menu-item v-for=\"childItem in item.children\" :key=\"childItem.path\" :index=\"childItem.path\" @click=\"clickMenu(childItem)\">\n         <el-icon>\n           <component :is=\"childItem.icon\"></component>\n         </el-icon>\n         <template #title>{{ childItem.label }}</template>\n       </el-menu-item>\n     </el-menu-item-group>\n   </el-sub-menu>\n </el-menu>\n</template>\n<script>\nimport * as ElementPlusIconsVue from '@element-plus/icons-vue';\nexport default {\n    data(){\n        return {\n            menu: [\n                {\n                    path: '/home',\n                    name: 'home',\n                    label: '首頁',\n                    icon: 'HomeFilled',\n                    url: 'Home/Home'\n                },\n                {\n                    path: '/mall',\n                    name: 'mall',\n                    label: '商品管理',\n                    icon: 'video-play',\n                    url: 'MallManage/MallManage'\n                },\n                {\n                    path: '/user',\n                    name: 'user',\n                    label: '用戶管理',\n                    icon: 'user',\n                    url: 'UserManage/UserManage'\n                },\n                {\n                    label: '其他',\n                    icon: 'location',\n                    children: [\n                        {\n                            path: '/page1',\n                            name: 'page1',\n                            label: '頁面1',\n                            icon: 'Setting',\n                            url: 'Other/PageOne'\n                        },\n                        {\n                            path: '/page2',\n                            name: 'page2',\n                            label: '頁面2',\n                            icon: 'Setting',\n                            url: 'Other/PageTwo'\n                        }\n                    ]\n                }\n              ]\n            }\n        },\n    methods: {\n        clickMenu(item) {\n          this.$router.push({ name: item.name});\n          this.$store.commit('selectMenu', item);\n        },\n        getIconComponent (iconName) {\n          return ElementPlusIconsVue[iconName] || 'defaultIcon';\n        }\n    },\n    computed: {\n        filterMenu() {\n            return this.menu.filter(item => !item.children);\n        },\n        filterMenu_withChildren() {\n            return this.menu.filter(item => item.children);\n        },\n        isCollapsed() {\n          return this.$store.state.tab.isCollapsed;\n        }\n    }\n};\n</script>\n<style lang=\"less\" scoped>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  height: 100%;\n}\n.el-aside {\n  border: none;\n  padding: 0;\n\n  ul {\n    height: 100%;\n    padding: 0;\n    margin: 0;\n  }\n\n  h3 {\n    color: #fff;\n    text-align: center;\n    line-height: 48px;\n    margin: 0 ;\n  }\n}\n</style>"],"mappings":";AAkCA,OAAO,KAAKA,mBAAkB,MAAO,yBAAyB;AAC9D,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,IAAI,EAAE,CACF;QACIC,IAAI,EAAE,OAAO;QACbC,IAAI,EAAE,MAAM;QACZC,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE,YAAY;QAClBC,GAAG,EAAE;MACT,CAAC,EACD;QACIJ,IAAI,EAAE,OAAO;QACbC,IAAI,EAAE,MAAM;QACZC,KAAK,EAAE,MAAM;QACbC