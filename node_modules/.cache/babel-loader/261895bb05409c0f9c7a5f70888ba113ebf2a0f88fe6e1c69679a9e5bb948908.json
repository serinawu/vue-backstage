hods: {\n            changeMenu(item) {\n                this.$router.push({ name: item.name })\n            },\n            handleClose(tag, index) {\n                const length = this.tags.length - 1;\n                if (tag.name !== this.$route.name ){\n                    return;\n                }\n                if (index === length) {\n                    this.$router.push({\n                        name: this.tags[index - 1].name\n                    })\n                } else {\n                    this.$router.push({\n                        name: this.tags[index].name\n                    })\n                }\n            },\n            closeTag(state, val) {\n                const result = state.tabList.findIndex(item => item.name === val.name);\n                state.tabList.splice(result, 1)\n            }\n        }\n    }   \n</script>\n<style lang=\"less\" scoped>\n    .tabs {\n        width: 100%;\n        display: flex;\n        align-items: center;\n        justify-content: flex-start;\n        padding: 20px 20px 5px;\n\n        .el-tag {\n            margin-right: 15px;\n            cursor: pointer;\n        }\n    }\n</style>"],"mappings":";AAgBA,SAASA,QAAO,QAAS,MAAM;AAC3B,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACH,OAAO,CAEP;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGH,QAAQ,CAAC;MACRI,IAAI,EAAEC,KAAI,IAAKA,KAAK,CAACC,GAAG,CAACC;IAC7B,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAACC,IAAI,EAAE;MACb,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEX,IAAI,EAAES,IAAI,CAACT;MAAK,CAAC;IACzC,CAAC;IACDY,WAAWA,CAACC,GAAG,EAAEC,KAAK,EAAE;MACpB,MAAMC,MAAK,GAAI,IAAI,CAACZ,IAAI,CAACY,MAAK,GAAI,CAAC;MACnC,IAAIF,GAAG,CAACb,IAAG,KAAM,IAAI,CAACgB,MAAM,CAAChB,IAAG,EAAG;QAC/B;MACJ;MACA,IAAIc,KAAI,KAAMC,MAAM,EAAE;QAClB,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC;UACdX,IAAI,EAAE,IAAI,CAACG,IAAI,CAACW,KAAI,GAAI,CAAC,CAAC,CAACd;QAC/B,CAAC;MACL,OAAO;QACH,IAAI,CAACU,OAAO,CAACC,IAAI,CAAC;UACdX,IAAI,EAAE,IAAI,CAACG,IAAI,CAACW,KAAK,CAAC,CAACd;QAC3B,CAAC;MACL;IACJ,CAAC;IACDiB,QAAQA,CAACb,K