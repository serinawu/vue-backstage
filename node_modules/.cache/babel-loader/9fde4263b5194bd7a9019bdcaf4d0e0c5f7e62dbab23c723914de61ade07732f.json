   :collapse=\"isCollapsed\"\r\n   background-color=\"#545c64\"\r\n   text-color=\"#fff\"\r\n   active-text-color=\"#ffd04b\"\r\n >\r\n <h3>{{ isCollapsed ? '後臺' : '通用後臺管理系統'}}</h3>\r\n   <el-menu-item v-for=\"item in filterMenu\" :index=\"item.path\" :key=\"item.path\" @click=\"clickMenu(item)\">\r\n     <el-icon>\r\n       <component :is=\"item.icon\"></component>\r\n     </el-icon>\r\n     <template #title>{{ item.label }}</template>\r\n   </el-menu-item>\r\n   <el-sub-menu v-for=\"item in filterMenu_withChildren\" :index=\"item.path\" :key=\"item.path\">\r\n     <template #title>\r\n       <el-icon>\r\n         <component :is=\"item.icon\"></component>\r\n       </el-icon>\r\n       <span>其他</span>\r\n     </template>\r\n     <el-menu-item-group>\r\n       <el-menu-item v-for=\"childItem in item.children\" :key=\"childItem.path\" :index=\"childItem.path\">\r\n         <el-icon>\r\n           <component :is=\"childItem.icon\"></component>\r\n         </el-icon>\r\n         <template #title>{{ childItem.label }}</template>\r\n       </el-menu-item>\r\n     </el-menu-item-group>\r\n   </el-sub-menu>\r\n </el-menu>\r\n</template>\r\n<script>\r\nimport * as ElementPlusIconsVue from '@element-plus/icons-vue';\r\nexport default {\r\n    data(){\r\n        return {\r\n            menu: [\r\n                {\r\n                    path: '/home',\r\n                    name: 'home',\r\n                    label: '首頁',\r\n                    icon: 'HomeFilled',\r\n                    url: 'Home/Home'\r\n                },\r\n                {\r\n                    path: '/mal',\r\n                    name: 'mall',\r\n                    label: '商品管理',\r\n                    icon: 'video-play',\r\n                    url: 'MallManage/MallManage'\r\n                },\r\n                {\r\n                    path: '/user',\r\n                    name: 'user',\r\n                    label: '用戶管理',\r\n                    icon: 'user',\r\n                    url: 'UserManage/UserManage'\r\n                },\r\n                {\r\n                    label: '其他',\r\n                    icon: 'location',\r\n                    children: [\r\n                        {\r\n                            path: '/page1',\r\n                            name: 'page1',\r\n                            label: '頁面1',\r\n                            icon: 'Setting',\r\n                            url: 'Other/PageOne'\r\n                        },\r\n                        {\r\n                            path: '/page2',\r\n                            name: 'page2',\r\n                            label: '頁面2',\r\n                            icon: 'Setting',\r\n                            url: 'Other/PageTwo'\r\n                        }\r\n                    ]\r\n                }\r\n              ]\r\n            }\r\n        },\r\n    methods: {\r\n        clickMenu(item) {\r\n          this.$router.push({\r\n            name: item.name,\r\n          })\r\n        },\r\n        getIconComponent (iconName) {\r\n\r\n          return ElementPlusIconsVue[iconName] || 'defaultIcon';\r\n        }\r\n    },\r\n    computed: {\r\n        filterMenu() {\r\n            return this.menu.filter(item => !item.children);\r\n        },\r\n        filterMenu_withChildren() {\r\n            return this.menu.filter(item => item.children);\r\n        },\r\n        isCollapsed() {\r\n          return this.$store.state.tab.isCollapsed;\r\n        }\r\n    }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.el-menu-vertical-demo:not(.el-menu--collapse) {\r\n  width: 200px;\r\n  height: 100%;\r\n}\r\n.el-aside {\r\n  border: none;\r\n  padding: 0;\r\n\r\n  ul {\r\n    height: 100%;\r\n    padding: 0;\r\n    margin: 0;\r\n  }\r\n\r\n  h3 {\r\n    color: #fff;\r\n    text-align: center;\r\n    line-height: 48px;\r\n    margin: 0 ;\r\n  }\r\n}\r\n</style>"],"mappings":";AAmCA,OAAO,KAAKA,mBAAkB,MAAO,yBAAyB;AAC9D,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,IAAI,EAAE,CACF