{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport * as ElementPlusIconsVue from '@element-plus/icons-vue';\nexport default {\n  data() {\n    return {\n      menu: []\n    };\n  },\n  methods: {\n    clickMenu(item) {\n      this.$router.push({\n        name: item.name\n      });\n      this.$store.commit('selectMenu', item);\n    },\n    getIconComponent(iconName) {\n      return ElementPlusIconsVue[iconName] || 'defaultIcon';\n    }\n  },\n  computed: {\n    filterMenu() {\n      return this.asyncMenu.filter(item => !item.children);\n    },\n    filterMenu_withChildren() {\n      return this.asyncMenu.filter(item => item.children);\n    },\n    isCollapsed() {\n      return this.$store.state.tab.isCollapsed;\n    },\n    asyncMenu() {\n      return this.$store.tab.menu;\n    }\n  }\n};","map":{"version":3,"names":["ElementPlusIconsVue","data","menu","methods","clickMenu","item","$router","push","name","$store","commit","getIconComponent","iconName","computed","filterMenu","asyncMenu","filter","children","filterMenu_withChildren","isCollapsed","state","tab"],"sources":["/Users/serina/Desktop/vue-backstage/src/components/CommonAside.vue"],"sourcesContent":["<template>\n  <el-menu\n   default-active=\"1\"\n   class=\"el-menu-vertical-demo\"\n   :collapse=\"isCollapsed\"\n   background-color=\"#545c64\"\n   text-color=\"#fff\"\n   active-text-color=\"#ffd04b\"\n >\n <h3>{{ isCollapsed ? '後臺' : '通用後臺管理系統'}}</h3>\n   <el-menu-item v-for=\"item in filterMenu\" :index=\"item.path\" :key=\"item.path\" @click=\"clickMenu(item)\">\n     <el-icon>\n       <component :is=\"item.icon\"></component>\n     </el-icon>\n     <template #title>{{ item.label }}</template>\n   </el-menu-item>\n   <el-sub-menu v-for=\"item in filterMenu_withChildren\" :index=\"item.path\" :key=\"item.path\">\n     <template #title>\n       <el-icon>\n         <component :is=\"item.icon\"></component>\n       </el-icon>\n       <span>其他</span>\n     </template>\n     <el-menu-item-group>\n       <el-menu-item v-for=\"childItem in item.children\" :key=\"childItem.path\" :index=\"childItem.path\" @click=\"clickMenu(childItem)\">\n         <el-icon>\n           <component :is=\"childItem.icon\"></component>\n         </el-icon>\n         <template #title>{{ childItem.label }}</template>\n       </el-menu-item>\n     </el-menu-item-group>\n   </el-sub-menu>\n </el-menu>\n</template>\n<script>\nimport * as ElementPlusIconsVue from '@element-plus/icons-vue';\nexport default {\n    data(){\n        return {\n            menu: []\n          }\n        },\n    methods: {\n        clickMenu(item) {\n          this.$router.push({ name: item.name});\n          this.$store.commit('selectMenu', item);\n        },\n        getIconComponent (iconName) {\n          return ElementPlusIconsVue[iconName] || 'defaultIcon';\n        }\n    },\n    computed: {\n        filterMenu() {\n            return this.asyncMenu.filter(item => !item.children);\n        },\n        filterMenu_withChildren() {\n            return this.asyncMenu.filter(item => item.children);\n        },\n        isCollapsed() {\n          return this.$store.state.tab.isCollapsed;\n        },\n        asyncMenu() {\n          return this.$store.tab.menu\n        }\n    }\n};\n</script>\n<style lang=\"less\" scoped>\n.el-menu-vertical-demo:not(.el-menu--collapse) {\n  width: 200px;\n  height: 100%;\n}\n.el-aside {\n  border: none;\n  padding: 0;\n\n  ul {\n    height: 100%;\n    padding: 0;\n    margin: 0;\n  }\n\n  h3 {\n    color: #fff;\n    text-align: center;\n    line-height: 48px;\n    margin: 0 ;\n  }\n}\n</style>"],"mappings":";AAmCA,OAAO,KAAKA,mBAAkB,MAAO,yBAAyB;AAC9D,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,IAAI,EAAE;IACR;EACF,CAAC;EACLC,OAAO,EAAE;IACLC,SAASA,CAACC,IAAI,EAAE;MACd,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAEH,IAAI,CAACG;MAAI,CAAC,CAAC;MACrC,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,YAAY,EAAEL,IAAI,CAAC;IACxC,CAAC;IACDM,gBAAeA,CAAGC,QAAQ,EAAE;MAC1B,OAAOZ,mBAAmB,CAACY,QAAQ,KAAK